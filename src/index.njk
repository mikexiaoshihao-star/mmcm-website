<!DOCTYPE html>
<html lang="zh-CN" class="scroll-smooth">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>MMCM STUDIO - INTEGRATED SYSTEM</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600&family=JetBrains+Mono:wght@300;400;500;600&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/assets/style.css">
    
    <script>
        tailwind.config = {
            theme: {
                extend: {
                    fontFamily: { sans: ['Inter', 'sans-serif'], mono: ['JetBrains Mono', 'monospace'] },
                    colors: { 'geek-black': '#050505', 'geek-dark': '#0a0a0a', 'geek-white': '#e5e5e5', 'geek-gray': '#666666', 'code-green': '#00ff41' },
                    backgroundImage: { 'grid-pattern': "linear-gradient(to right, #222 1px, transparent 1px), linear-gradient(to bottom, #222 1px, transparent 1px)" },
                    animation: { 'grid-flow': 'gridMove 60s linear infinite' },
                    keyframes: { gridMove: { '0%': { backgroundPosition: '0 0' }, '100%': { backgroundPosition: '4rem 4rem' } } }
                }
            }
        }
    </script>

    <script>
        const siteData = {
            categories: {{ site.categories | dump | safe }},
            homeProjects: {{ site.homeProjects | dump | safe }},
            projects: {{ site.projects | dump | safe }}
        };

        document.addEventListener('alpine:init', () => {
            Alpine.data('scrambleText', (initialText = '') => ({
                displayText: initialText, 
                chars: 'ABCDEFGHIJKLMNOPQRSTUVWXYZ/<>[]#1234567890_@$%',
                interval: null,
                init() { if (initialText) this.animate(initialText); },
                animate(newText) {
                    const target = newText || '';
                    let iteration = 0;
                    clearInterval(this.interval);
                    this.interval = setInterval(() => {
                        this.displayText = target.split('').map((letter, index) => {
                            if(index < iteration) return target[index];
                            return this.chars[Math.floor(Math.random() * this.chars.length)];
                        }).join('');
                        if(iteration >= target.length){ clearInterval(this.interval); this.displayText = target; }
                        iteration += 1/3; 
                    }, 20); 
                }
            }));

            Alpine.data('categoryBtn', (key, name) => ({
                key: key, name: name, displayText: key,
                chars: 'ABCDEFGHIJKLMNOPQRSTUVWXYZ/<>[]#1234567890_@$%',
                interval: null,
                init() { this.animate(this.key); },
                animate(newText) {
                    const target = newText || '';
                    let iteration = 0;
                    clearInterval(this.interval);
                    this.interval = setInterval(() => {
                        this.displayText = target.split('').map((letter, index) => {
                            if(index < iteration) return target[index];
                            return this.chars[Math.floor(Math.random() * this.chars.length)];
                        }).join('');
                        if(iteration >= target.length){ clearInterval(this.interval); this.displayText = target; }
                        iteration += 1/3; 
                    }, 20); 
                }
            }));

            Alpine.data('mainApp', () => ({
                currentView: 'home',
                activeFilter: 'ALL',
                currentHomeIndex: 0,
                homeImgVisible: true,
                categories: siteData.categories,
                homeProjects: siteData.homeProjects,
                projects: siteData.projects,
                selectedProject: null,
                detailImages: [],
                currentDetailImageIndex: 0,
                isImageLoading: false,

                get currentHomeProject() { return this.homeProjects[this.currentHomeIndex]; },
                nextHomeProject() {
                    this.homeImgVisible = false;
                    setTimeout(() => { this.currentHomeIndex = (this.currentHomeIndex + 1) % this.homeProjects.length; this.homeImgVisible = true; }, 400);
                },
                switchView(view) { window.scrollTo({ top: 0, behavior: 'smooth' }); this.currentView = view; },
                openProject(project) {
                    this.selectedProject = project;
                    this.currentDetailImageIndex = 0;
                    this.detailImages = [
                        { url: project.url, desc: 'MAIN_VIEW', meta: 'RAW // 24MP' },
                        { url: project.url, desc: 'DETAIL_01', meta: 'ISO 200 // F8' },
                        { url: project.url, desc: 'CONTEXT_01', meta: 'ISO 400 // F4' }
                    ];
                    this.switchView('project_detail');
                },
                selectDetailImage(index) {
                    if (this.currentDetailImageIndex === index) return;
                    this.isImageLoading = true;
                    setTimeout(() => { this.currentDetailImageIndex = index; this.isImageLoading = false; }, 200);
                },
                prevDetailImage() {
                    if (this.detailImages.length === 0) return;
                    this.isImageLoading = true;
                    setTimeout(() => { this.currentDetailImageIndex = (this.currentDetailImageIndex - 1 + this.detailImages.length) % this.detailImages.length; this.isImageLoading = false; }, 200);
                },
                nextDetailImage() {
                    if (this.detailImages.length === 0) return;
                    this.isImageLoading = true;
                    setTimeout(() => { this.currentDetailImageIndex = (this.currentDetailImageIndex + 1) % this.detailImages.length; this.isImageLoading = false; }, 200);
                },
                get filteredProjects() {
                    if (this.activeFilter === 'ALL') return this.projects;
                    return this.projects.filter(p => p.category === this.activeFilter);
                }
            }));
        });
    </script>
    <script defer src="https://cdn.jsdelivr.net/npm/alpinejs@3.x.x/dist/cdn.min.js"></script>
</head>
<body class="bg-geek-black text-geek-white font-sans antialiased selection:bg-geek-white selection:text-geek-black relative min-h-svh overflow-x-hidden flex flex-col" x-data="mainApp">

    <!-- 背景特效 -->
    <div class="fixed inset-0 z-0 pointer-events-none bg-[size:4rem_4rem] bg-grid-pattern opacity-[0.2] animate-grid-flow"></div>
    <div class="fixed inset-0 z-0 pointer-events-none scanlines z-[1] opacity-30"></div>
    <div class="fixed inset-0 z-0 pointer-events-none vignette z-[2]"></div>

    <!-- 顶部导航栏 -->
    <nav class="fixed top-0 w-full z-40 h-16 md:h-20 flex items-center justify-between px-4 md:px-12 bg-geek-black/80 border-b border-white/10 backdrop-blur-md">
        <a href="#" @click.prevent="switchView('home')" class="flex flex-col items-start group">
            <span class="font-mono font-bold tracking-tighter leading-none text-lg md:text-2xl text-white" x-data="scrambleText('MMCM STUDIO')" @mouseenter="animate('MMCM STUDIO')" x-text="displayText"></span>
            <div class="w-full flex justify-between font-sans text-[8px] md:text-xs text-geek-gray tracking-widest uppercase">
                <span>毫</span><span>厘</span><span>建</span><span>筑</span><span>摄</span><span>影</span>
            </div>
        </a>
        <div class="flex gap-6 md:gap-12 font-mono text-[10px] md:text-xs text-gray-400">
            <a href="#" @click.prevent="switchView('works')" :class="currentView === 'works' || currentView === 'project_detail' ? 'text-white' : ''" class="transition-colors" x-data="scrambleText('WORKS')" @mouseenter="animate('WORKS')" x-text="displayText">WORKS</a>
            <a href="#" @click.prevent="switchView('studio')" :class="currentView === 'studio' ? 'text-white' : ''" class="transition-colors" x-data="scrambleText('STUDIO')" @mouseenter="animate('STUDIO')" x-text="displayText">STUDIO</a>
            <a href="#" @click.prevent="switchView('contact')" :class="currentView === 'contact' ? 'text-white' : ''" class="transition-colors" x-data="scrambleText('CONTACT')" @mouseenter="animate('CONTACT')" x-text="displayText">CONTACT</a>
        </div>
    </nav>

    <!-- VIEW 1: HOME -->
    <section x-show="currentView === 'home'" class="h-svh w-full flex flex-col pt-16 md:pt-20 pb-8 px-4 md:px-12 relative z-10">
        <div class="flex-grow w-full relative overflow-hidden bg-geek-dark border border-white/10 group cursor-pointer" @click="nextHomeProject()">
            <img x-show="homeImgVisible" :src="currentHomeProject.img" class="w-full h-full object-cover opacity-80 group-hover:opacity-100 transition-all duration-700 absolute inset-0" x-transition:enter="transition ease-out duration-500" x-transition:enter-start="opacity-0" x-transition:enter-end="opacity-80">
        </div>
        <div class="mt-6 flex flex-col md:flex-row justify-between items-start font-mono text-[10px] md:text-xs">
            <div class="flex flex-col gap-1 items-start mb-4 md:mb-0">
                <div class="text-gray-500 h-5 overflow-visible whitespace-nowrap" x-data="scrambleText()" x-effect="animate(currentHomeProject.architect)" x-text="displayText"></div>
                <div class="text-white font-bold text-sm md:text-xl" x-data="scrambleText()" x-effect="animate(currentHomeProject.title)" x-text="displayText"></div>
            </div>
            <div class="flex flex-col gap-1 items-start md:items-end mt-0">
                <div class="text-gray-500 h-5 overflow-visible whitespace-nowrap" x-data="scrambleText()" x-effect="animate(currentHomeProject.address)" x-text="displayText"></div>
                <div class="flex items-center text-code-green font-bold h-5 overflow-visible whitespace-nowrap">
                    <span class="w-1.5 h-1.5 rounded-full bg-code-green animate-pulse shadow-[0_0_8px_#00ff41] mr-2"></span>
                    <span x-data="scrambleText()" x-effect="animate(currentHomeProject.coords)" x-text="displayText"></span>
                </div>
            </div>
        </div>
    </section>

    <!-- VIEW 2: WORKS -->
    <section x-show="currentView === 'works'" x-cloak class="px-4 md:px-12 pt-24 md:pt-32 pb-20 max-w-screen-2xl mx-auto z-10 w-full min-h-screen">
        <div class="mb-10 flex flex-wrap gap-3 font-mono text-xs z-20 relative">
            <button @click="activeFilter = 'ALL'" 
                    class="relative px-3 py-2 border transition-all duration-300 group overflow-hidden"
                    :class="activeFilter === 'ALL' ? 'bg-code-green border-code-green text-black font-bold' : 'bg-transparent border-white/20 text-gray-500 hover:border-white hover:text-white'"
                    x-data="scrambleText('ALL_PROJECTS')" @mouseenter="animate('ALL_PROJECTS')">
                <span class="relative z-10 flex items-center gap-2">
                    <span class="w-1.5 h-1.5 rounded-sm transition-colors duration-300" :class="activeFilter === 'ALL' ? 'bg-black' : 'bg-gray-600 group-hover:bg-code-green'"></span>
                    <span x-text="displayText"></span>
                </span>
            </button>
            <template x-for="cat in categories" :key="cat.key">
                <button @click="activeFilter = cat.key" 
                        class="relative px-3 py-2 border transition-all duration-300 group overflow-hidden"
                        :class="activeFilter === cat.key ? 'bg-code-green border-code-green text-black font-bold' : 'bg-transparent border-white/20 text-gray-500 hover:border-white hover:text-white'"
                        x-data="categoryBtn(cat.key, cat.name)" @mouseenter="animate(key)">
                    <span class="relative z-10 flex items-center gap-2">
                        <span class="w-1.5 h-1.5 rounded-sm transition-colors duration-300" :class="activeFilter === key ? 'bg-black' : 'bg-gray-600 group-hover:bg-code-green'"></span>
                        <span x-text="displayText"></span>
                        <span class="opacity-50 text-[10px] scale-90" x-text="'// ' + name"></span>
                    </span>
                </button>
            </template>
        </div>
        <div class="columns-1 md:columns-3 gap-8">
            <template x-for="(project, index) in filteredProjects" :key="index">
                <div class="group relative cursor-pointer flex flex-col gap-3 break-inside-avoid mb-8" @click="openProject(project)">
                    <div class="relative w-full bg-geek-dark overflow-hidden border border-white/10 group-hover:border-code-green transition-colors duration-300">
                        <img :src="project.url" class="w-full h-auto block filter grayscale group-hover:grayscale-0 transition-all duration-500">
                    </div>
                    <div class="font-mono text-[10px] flex flex-col gap-1 border-l border-white/10 pl-3 transition-colors duration-300 group-hover:border-code-green/50">
                        <div class="flex justify-between items-baseline">
                            <div class="text-white font-bold group-hover:text-code-green transition-colors">
                                <span x-text="project.title"></span><span class="mx-1">|</span><span x-text="project.architect"></span>
                            </div>
                            <div class="text-gray-600 scale-90" x-text="'[ ' + project.year + ' ]'"></div>
                        </div>
                        <div class="flex items-center gap-2 mt-1">
                            <span class="bg-white/10 px-1.5 py-0.5 rounded text-[9px] text-gray-300" x-text="project.category"></span>
                            <span class="w-1.5 h-1.5 bg-code-green rounded-full animate-pulse"></span>
                            <span class="text-code-green text-[9px]" x-text="project.coords"></span>
                        </div>
                    </div>
                </div>
            </template>
        </div>
    </section>

    <!-- VIEW 2.5: PROJECT DETAIL -->
    <section x-show="currentView === 'project_detail'" x-cloak class="fixed inset-0 z-[60] bg-geek-black flex flex-col font-mono">
        <div class="lg:hidden fixed top-0 left-0 w-full h-16 flex items-center px-4 bg-geek-black/90 backdrop-blur-md border-b border-white/10 z-[70]">
            <button @click="switchView('works')" class="w-10 h-10 flex items-center justify-start text-code-green text-xl font-light hover:text-white transition-colors">《</button>
            <div class="flex-grow text-center text-[10px] text-gray-400 tracking-widest uppercase pr-10">Project Detail</div>
        </div>
        <div class="relative flex-grow w-full bg-geek-dark overflow-hidden flex items-center justify-center pt-16 lg:pt-0 group">
            <button @click.stop="prevDetailImage()" class="absolute left-2 md:left-8 z-30 w-12 h-12 flex items-center justify-center text-code-green hover:text-white text-3xl font-light focus:outline-none transition-colors bg-black/20 hover:bg-black/50 backdrop-blur-sm rounded-full">〈</button>
            <div class="relative w-full h-full p-4 lg:p-8 flex items-center justify-center">
                <div x-show="isImageLoading" class="absolute inset-0 flex items-center justify-center bg-black z-20"><span class="text-code-green animate-pulse text-xs">DECODING...</span></div>
                <img :src="detailImages[currentDetailImageIndex]?.url" class="max-w-full max-h-full object-contain shadow-2xl transition-opacity duration-300" :class="isImageLoading ? 'opacity-0' : 'opacity-100'" alt="Detail View">
            </div>
            <button @click.stop="nextDetailImage()" class="absolute right-2 md:right-8 z-30 w-12 h-12 flex items-center justify-center text-code-green hover:text-white text-3xl font-light focus:outline-none transition-colors bg-black/20 hover:bg-black/50 backdrop-blur-sm rounded-full">〉</button>
        </div>
        <div class="w-full h-auto min-h-[160px] lg:h-48 bg-black border-t border-white/5 flex flex-col lg:flex-row flex-shrink-0 z-50">
            <div class="w-full lg:w-1/4 p-4 lg:p-6 border-b lg:border-b-0 lg:border-r border-white/5 flex flex-col justify-between">
                <div class="hidden lg:block"><button @click="switchView('works')" class="text-xs text-code-green hover:text-white flex items-center gap-2 mb-4 transition-colors"><span>《</span> <span>RETURN TO GRID</span></button></div>
                <div>
                    <h2 class="text-base lg:text-xl text-white font-bold leading-tight truncate" x-text="selectedProject?.title"></h2>
                    <div class="flex justify-between text-[10px] text-gray-500 mt-2"><span x-text="selectedProject?.architect"></span><span x-text="selectedProject?.year"></span></div>
                </div>
            </div>
            <div class="flex-grow p-4 overflow-x-auto no-scrollbar flex items-center gap-4">
                <template x-for="(img, index) in detailImages" :key="index">
                    <div @click="selectDetailImage(index)" class="flex-shrink-0 w-24 md:w-32 flex flex-col cursor-pointer transition-all group/thumb" :class="currentDetailImageIndex === index ? 'opacity-100' : 'opacity-50 hover:opacity-80'">
                        <div class="w-full aspect-video bg-gray-800 border overflow-hidden relative mb-2" :class="currentDetailImageIndex === index ? 'border-code-green' : 'border-white/10'">
                            <img :src="img.url" class="w-full h-full object-cover">
                            <div class="absolute inset-0 bg-code-green/20" x-show="currentDetailImageIndex === index"></div>
                        </div>
                        <div class="flex flex-col">
                            <div class="text-[9px] font-bold text-gray-300 group-hover/thumb:text-code-green transition-colors truncate" x-text="img.desc"></div>
                            <div class="text-[8px] text-gray-600 font-mono mt-0.5 tracking-tighter" x-text="img.meta"></div>
                        </div>
                    </div>
                </template>
            </div>
        </div>
    </section>

    <!-- VIEW 3: STUDIO -->
    <section x-show="currentView === 'studio'" x-cloak class="min-h-screen w-full relative z-10 bg-geek-black">
        <div class="w-full flex flex-col md:flex-row pt-20">
            <div class="w-full md:w-[30%] md:h-[calc(100vh-5rem)] md:sticky md:top-20 flex flex-col items-center justify-start border-r border-white/5 bg-geek-black z-20 pt-12 md:pt-16 pb-12">
                <div class="w-[200px] lg:w-[240px] aspect-[3/4] mb-6 overflow-hidden shadow-2xl">
                    <img src="https://images.unsplash.com/photo-1507003211169-0a1dd7228f2d?q=80&w=800&auto=format&fit=crop" class="w-full h-full object-cover filter grayscale contrast-110" alt="M.C. CHEN">
                </div>
                <div class="text-center font-mono">
                    <h2 class="text-xl font-bold text-white tracking-widest">M. C. CHEN</h2>
                    <div class="text-[10px] text-gray-500 mt-2 tracking-widest">LEAD PHOTOGRAPHER</div>
                </div>
            </div>
            <div class="w-full md:w-[70%] px-6 md:px-24 pt-12 md:pt-16 pb-32 space-y-20">
                <div>
                    <div class="text-code-green font-mono text-xs mb-6 flex items-center gap-2"><span class="w-2 h-2 bg-code-green"></span>01 // PHOTOGRAPHER</div>
                    <div class="font-mono text-sm text-gray-400 leading-loose max-w-xl space-y-6 border-l border-white/10 pl-6">
                        <p>我们拒绝将建筑摄影视为简单的记录。在 MMCM，摄影是一次解剖手术——我们将物理空间还原为最原始的几何算法。</p>
                        <p>光线不是目的，而是手段。我们利用阴影切割实体，利用过曝消除杂质，旨在揭示隐藏在混凝土与钢筋之下的数学之美与沉默张力。</p>
                    </div>
                </div>
                <div>
                    <div class="text-code-green font-mono text-xs mb-6 flex items-center gap-2"><span class="w-2 h-2 bg-code-green"></span>02 // THE STUDIO</div>
                    <div class="font-mono text-sm text-gray-300 leading-relaxed max-w-xl">
                        <p class="mb-4"><strong class="text-white bg-white/10 px-1">MMCM STUDIO</strong> 成立于数字与实体的交界处。我们是一个由建筑师、程序员和视觉艺术家组成的跨学科团队。</p>
                        <p>我们的工作流融合了传统的大画幅摄影纪律与现代的计算生成逻辑。我们不服务于平庸的视觉快消品，只为那些敢于挑战结构极限的建筑作品提供视觉档案。</p>
                    </div>
                </div>
                <div>
                    <div class="text-code-green font-mono text-xs mb-8 flex items-center gap-2"><span class="w-2 h-2 bg-code-green"></span>03 // CLIENT</div>
                    <div class="font-mono text-sm w-full border-t border-white/10">
                        <template x-for="(client, index) in [{name: 'SOM', loc: 'US/CN'}, {name: 'FOSTER + PARTNERS', loc: 'UK'}, {name: 'ZAHA HADID ARCHITECTS', loc: 'UK'}, {name: 'MAD ARCHITECTS', loc: 'CN'}, {name: 'KENGO KUMA', loc: 'JP'}, {name: 'GENSLER', loc: 'US'}, {name: 'TADAO ANDO', loc: 'JP'}, {name: 'OPEN ARCHITECTURE', loc: 'CN'}, {name: 'SNØHETTA', loc: 'NO'}, {name: 'HEATHERWICK STUDIO', loc: 'UK'}]">
                            <div class="group flex justify-between items-center py-3 border-b border-white/10 hover:bg-white/5 transition-colors cursor-crosshair px-2">
                                <div class="flex items-center gap-4"><span class="text-gray-700 text-xs" x-text="String(index + 1).padStart(2, '0')"></span><span class="text-gray-400 group-hover:text-white group-hover:translate-x-2 transition-all duration-300 font-bold" x-text="client.name"></span></div>
                                <div class="flex items-center gap-4 opacity-50 group-hover:opacity-100 transition-opacity duration-300"><span class="text-[10px] text-gray-500 group-hover:text-code-green" x-text="client.loc"></span><span class="text-code-green text-xs opacity-0 group-hover:opacity-100 transition-opacity">>></span></div>
                            </div>
                        </template>
                    </div>
                    <div class="mt-8 text-[10px] text-gray-600 font-mono flex justify-end items-center gap-2"><span class="w-2 h-2 bg-gray-800 rounded-full"></span>DATABASE_LAST_UPDATED: 2026.01.07</div>
                </div>
            </div>
        </div>
    </section>

    <!-- VIEW 4: CONTACT -->
    <section x-show="currentView === 'contact'" x-cloak class="min-h-screen pt-24 md:pt-40 px-6 md:px-12 z-10 flex justify-center">
        <div class="w-full max-w-4xl">
            <div class="mb-12 font-mono text-[10px] text-code-green flex items-center gap-2"><span class="w-1.5 h-1.5 bg-code-green animate-pulse"></span> SIGNAL_STABLE</div>
            <div class="grid grid-cols-1 md:grid-cols-2 gap-16">
                <div class="space-y-8">
                    <div>
                        <div class="text-[9px] text-gray-600 font-mono mb-1 uppercase tracking-widest">Mobile / Wechat <span class="opacity-50 ml-1">// 手机/微信</span></div>
                        <div class="text-xl md:text-3xl text-white font-mono">+86 139 0000 0000</div>
                    </div>
                    <div>
                        <div class="text-[9px] text-gray-600 font-mono mb-1 uppercase tracking-widest">Email Address <span class="opacity-50 ml-1">// 电子邮箱</span></div>
                        <a href="mailto:hello@mmcm.studio" class="text-xl md:text-3xl text-white font-mono hover:text-code-green transition-colors">hello@mmcm.studio</a>
                    </div>
                    <div>
                        <div class="text-[9px] text-gray-600 font-mono mb-1 uppercase tracking-widest">Official Site <span class="opacity-50 ml-1">// 官方网站</span></div>
                        <a href="#" class="text-xl md:text-3xl text-white font-mono hover:underline decoration-code-green underline-offset-4 transition-all">www.mmcm.studio</a>
                    </div>
                </div>
                <div class="space-y-4">
                    <div class="text-xs text-gray-500 font-mono border-b border-white/10 pb-2 mb-4">02 // SOCIAL_MATRIX</div>
                    <template x-for="social in [{n:'XIAOHONGSHU', cn:'小红书', id:'MMCM_Space'}, {n:'INSTAGRAM', cn:'INS', id:'@mmcm.studio'}, {n:'WECHAT_OFFICIAL', cn:'公众号', id:'MMCM建筑摄影'}, {n:'WEIBO', cn:'微博', id:'@MMCM_Studio'}]">
                        <div class="flex justify-between items-center py-4 border-b border-white/5 group">
                            <div class="font-mono">
                                <div class="text-[9px] text-gray-500 uppercase mb-1 group-hover:text-white transition-colors"><span x-text="social.n"></span><span class="opacity-50 ml-1" x-text="'// ' + social.cn"></span></div>
                                <div class="text-xs text-white" x-text="social.id"></div>
                            </div>
                            <span class="text-[9px] text-code-green opacity-0 group-hover:opacity-100 transition-opacity">>> LINK</span>
                        </div>
                    </template>
                </div>
            </div>
